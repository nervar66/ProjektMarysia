<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.pcz.icis.budzetdomowy" >
    <!--<uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />-->
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"

        android:theme="@style/AppTheme" >
        <activity
            android:name=".main"
            android:label="@string/app_name" >
            <intent-filter>
               <!-- <action android:name="android.intent.action.MAIN" />-->
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".splashscreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dochody"
            android:label="@string/app_name">
            <intent-filter >
                <action  android:name="in.wptrafficanalyzer.dochody"/>
                <category  android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".wydatki"
            android:label="@string/app_name">
            <intent-filter >
                <action  android:name="in.wptrafficanalyzer.wydatki"/>
                <category  android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".edytuj"
            android:label="@string/app_name">
            <intent-filter >
                <action  android:name="in.wptrafficanalyzer.edycja"/>
                <category  android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".pokaz"
            android:label="@string/app_name">
            <intent-filter >
                <action  android:name="in.wptrafficanalyzer.pokaz"/>
                <category  android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
